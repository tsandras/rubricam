<%= simple_form_for(@objet) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :niveau %>
    <%= f.input :type %>
    <%= f.input :resonnance %>
    <%= f.input :personnage_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
